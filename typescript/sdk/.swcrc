{
  "minify": true,
  "jsc": {
    "loose": true,
    "parser": {
      "syntax": "typescript",
      "tsx": false
    },
    "target": "es2020",
    "minify": {
      "compress": {
        "unused": true
      },
      "mangle": true
    }
  },
  "module": {
    "type": "commonjs",
    "strict": true,
    "noInterop": true
  }
}
